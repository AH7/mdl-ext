{
  "name": "mdl-ext",
  "version": "0.0.0-semantically-released",
  "description": "Components based on the Google Material Design Lite framework",
  "main": "lib/index.js",
  "scripts": {
    "commit": "git-cz",
    "dev": "./node_modules/.bin/webpack --progress --colors --watch --mode=dev",
    "eslint": "./node_modules/.bin/eslint ./src",
    "jshint": "./node_modules/.bin/jshint ./src",
    "test": "./node_modules/.bin/mocha  -w --opts ./mocha.opts './test/**/*.spec.js'",
    "test:coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/istanbul cover --root ./src ./node_modules/.bin/_mocha -- --opts ./mocha.opts  -r .istanbul-mocha.config.js './test/**/*.spec.js'",
    "check-coverage": "./node_modules/.bin/istanbul check-coverage --statements 85 --branches 70 --functions 100 --lines 90",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/.bin/codecov",
    "prebuild": "rm -rf lib && ./node_modules/.bin/webpack",
    "build": "./node_modules/.bin/webpack --mode=build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run build && npm run check-coverage"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "czConfig": {
    "path": "./node_modules/cz-conventional-changelog"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leifoolsen/mdl-ext.git"
  },
  "keywords": [
    "mdl",
    "material-design-lite",
    "es6",
    "es7",
    "EcmaScript 2015",
    "EcmaScript 2016",
    "css",
    "sass",
    "webpack"
  ],
  "author": "Leif Olsen (https://github.com/leifoolsen)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/leifoolsen/mdl-ext/issues"
  },
  "homepage": "https://github.com/leifoolsen/mdl-ext#readme",
  "peerDependencies": {
    "material-design-icons": ">= 2.2.0",
    "material-design-lite": ">= 1.1.0 < 2"
  },
  "devDependencies": {
    "autoprefixer": "6.3.3",
    "babel-cli": "6.6.5",
    "babel-core": "6.6.5",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-syntax-decorators": "6.5.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-polyfill": "6.6.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.6.1",
    "chai": "3.5.0",
    "chalk": "1.1.1",
    "commitizen": "2.6.3",
    "copy-webpack-plugin": "1.1.1",
    "css-loader": "0.23.1",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "2.2.0",
    "eslint-loader": "1.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "gulp": "3.9.1",
    "gulp-git": "1.7.0",
    "ignore-styles": "1.2.0",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "8.1.0",
    "jsdomify": "2.0.1",
    "jshint": "2.9.1",
    "mocha": "2.4.5",
    "node-sass": "3.4.2",
    "null-loader": "0.1.1",
    "postcss-loader": "0.8.1",
    "require-uncached": "1.0.2",
    "resolve-url-loader": "1.4.3",
    "roboto-fontface": "^0.4.5",
    "sass-loader": "3.1.2",
    "sinon": "1.17.3",
    "style-loader": "0.13.0",
    "stylelint": "4.4.0",
    "stylelint-config-suitcss": "4.0.0",
    "stylelint-loader": "0.2.4",
    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1",
    "yargs": "4.2.0",
    "semantic-release": "^4.3.5"
  }
}
